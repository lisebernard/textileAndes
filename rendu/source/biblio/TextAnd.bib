@article{cerecedaSemiologieTissusAndins1978,
  title = {Sémiologie Des Tissus Andins : Les Talegas d'{{Isluga}}},
  shorttitle = {Sémiologie Des Tissus Andins},
  author = {Cereceda, Verónica},
  date = {1978-12},
  journaltitle = {Annales. Histoire, Sciences Sociales},
  shortjournal = {Annales},
  volume = {33},
  number = {5-6},
  pages = {1017--1035},
  issn = {0395-2649, 1953-8146},
  doi = {10.3406/ahess.1978.293997},
  url = {https://www.cambridge.org/core/product/identifier/S0395264900151522/type/journal_article},
  urldate = {2022-09-27},
  abstract = {Les textiles andins, surtout anciens, ont suscité de nombreuses études qui les abordent, pour ainsi dire, de l'extérieur : elles traitent des problèmes de fabrication et d'utilisation, de chronologie, de distribution spatiale et culturelle. De telles études cherchent des réponses, en quelque sorte, « au-delà du tissu » : du point de vue iconographique, elles s'efforcent d'identifier les dessins figuratifs (flore, faune, scènes mythiques), ou d'interpréter les formes abstraites. Mais que faire quand nous nous trouvons en présence d'un paysage étrange fait de bandes et de rayures, ou, pis encore, d'espaces vides ? Nous connaissons parfois certains éléments du lexique (” tête », « soleil », etc.), mais nous ignorons leur syntaxe, les articulations des unités entre elles, et les relations des parties au tout.           ,              Summary                            The present article attempts to analyze the language of Andean fabrics on the basis of the example of the               talegas               woven by the women of Isluga (Chile). These are bags or sacks measuring from 20 to 50 cm which serve a number of uses, both daily and ceremonial (the transport of seeds and food during the journeys of the living and the dead).                                         The woven space is highly structured: square in form, the sacks are decorated with bands and stripes (in natural colors or dyed) which create a central axis, called the “heart”. This axis divides the sack into two symmetrical halves, which constitute the “body” of the               talega               . The extremities of each side, which are always brown, represent “mouths”. Within each half, a black band, contrasting with a bright band, creates a secondary axis and symbolically represents the dialectic of light and dark, of masculine and feminine.                          The sack is thus conceived as a living being, an animal, drawn according to the model of “duplicated representation” analyzed by C. Lévi-Strauss. The ensemble of bands and stripes constitutes a textile code which transmits a message concerning fertility.},
  langid = {english},
  keywords = {Textile}
}

@article{conklinStructureMeaningAndean1997,
  title = {Structure as {{Meaning}} in {{Andean Textiles}}},
  author = {Conklin, William J.},
  date = {1997},
  journaltitle = {Chungara: Revista de Antropología Chilena},
  volume = {29},
  number = {1},
  eprint = {27802053},
  eprinttype = {jstor},
  pages = {109--131},
  publisher = {{Universidad de Tarapaca}},
  issn = {07161182},
  url = {http://www.jstor.org/stable/27802053},
  urldate = {2022-03-10},
  abstract = {Ordinary thought in the non-Andean Western world generally sees a clear distinction between the art of an object and the support structure used for that art. This paper suggests that this was not so in the ancient Andean world, that art and technology were not separate, and uses textiles as the prime example. The broad importance in the Andes of fabric technology is noted. In addition to garment creation, the symbolic value of textiles is noted and the uses of fabric technology in architecture and in warfare are noted. The sculptural representation of textiles in Tiwanaku is pointed out. The identification of individual Andean cultures with special textile structures is described. Examples of textile structure that carry religious meaning are cited as are the uses of string textiles to carry complex messages. The uses of a design motif that represents textile structure in the Altiplano is examined in a two millennia long series of images ranging from Pucara to Inka. The paper in general notes that the whole truth about the subject will never be known. En el pensamiento no andino occidental se establece, normalmente, una clara separaci\&\#xf3;n entre lo art\&\#xed;stico de un objeto y el soporte estructural usado para su creaci\&\#xf3;n. En este art\&\#xed;culo sugerimos que esto no ocurri\&\#xf3; en el mundo andino precolombino, que el arte y la tecnolog\&\#xed;a empleados para su realizaci\&\#xf3;n no estaban separados y para ello se presentan casos de la textiler\&\#xed;a, como ejemplos primordiales. La amplia importancia de la tecnolog\&\#xed;a textil en los Andes se pone de relieve. Adem\&\#xe1;s de ser piezas de vestir, el valor simb\&\#xf3;lico de los tejidos es reconocido como as\&\#xed; tambi\&\#xe9;n su aplicaci\&\#xf3;n tecnol\&\#xf3;gica en la arquitectura y en las pr\&\#xe1;cticas guerreras. Se enfatizan las representaciones esculturales de los textiles Tiwanaku. Se describen algunas culturas andinas identificadas con estructuras textiles propias. Se presentan ejemplos de estructuras textiles con significado religioso, como as\&\#xed; tambi\&\#xe9;n el uso de fajas textiles portadoras de complejos mensajes. Los usos del dise\&\#xf1;o de un motivo que representa estructuras textiles en el altiplano se analizan a trav\&\#xe9;s de una larga serie de im\&\#xe1;genes desde Pukara hasta el Inka. En el art\&\#xed;culo hacemos notar que muchas de las representaciones de estos objetos no podr\&\#xe1;n ser nunca conocidas en su totalidad.},
  keywords = {Textile},
  file = {/Users/lise/Zotero/storage/PHQJE5Q4/Conklin - 1997 - Structure as Meaning in Andean Textiles.pdf}
}

