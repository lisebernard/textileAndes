@article{cardonRevancheNeuronesInvention2018,
  title = {La revanche des neurones. L’invention des machines inductives et la controverse de l’intelligence artificielle},
  shorttitle = {La revanche des neurones},
  author = {Cardon, Dominique and Cointet, Jean-Philippe and Mazières, Antoine},
  date = {2018},
  journaltitle = {Réseaux},
  shortjournal = {Réseaux},
  volume = {211},
  number = {5},
  pages = {173--220},
  publisher = {{La Découverte}},
  location = {{Paris}},
  issn = {0751-7971},
  doi = {10.3917/res.211.0173},
  url = {https://www.cairn.info/revue-reseaux-2018-5-page-173.htm},
  urldate = {2023-06-11},
  abstract = {Sous le nom d’Intelligence artificielle, de nouvelles techniques de calcul numérique assurent qu’elles peuvent prédire l’avenir. Les choix de consommation, les goûts culturels, les déplacements ou les comportements des internautes sont de plus en plus soumis aux calculs de ces nouvelles machines à prédire. Ce dossier de Réseaux propose d’étudier la manière dont cette nouvelle forme de quantification pénètre progressivement de nombreux secteurs d’activité : les services offerts par les nouvelles plateformes numériques, les décisions judiciaires, la surveillance épidémiologique, les recommandations commerciales, les opérateurs financiers, les grands systèmes métrologiques de la ville, du transport ou du climat.  Les articles réunis dans ce dossier proposent d’abord, dans une perspective d’histoire sociale des sciences et des techniques, d’explorer les conditions qui ont rendu possible le développement des méthodes d’apprentissage automatique (machine learning) qui sont au cœur des machines à prédire. En s’appuyant sur des bases de données massives, elles permettent d’apprendre “automatiquement” des structures caractéristiques dans des bases de données massives afin d’en induire des classifications (par la reconnaissance de patterns, la détection de clusters ou l’induction de modèle). Comment ces techniques de prédiction s’inscrivent-elles dans l’histoire des sciences et des techniques et, notamment, dans l’histoire longue et tumultueuse de l’Intelligence artificielle ? C’est ensuite à la manière dont ces machines sont conçues et fabriquées que s’intéresse ce dossier en prêtant attention au travail des ingénieurs et des informaticiens qui les programmes. Enfin, les articles de ce dossier de Réseaux  s’attache à observer leur mise en oeuvre dans des champs très différents celui de la surveillance des comportements, de la prédiction du comportement sexuel ou des recommandations commerciales. À travers le calcul de bases de données massives, les nouvelles techniques de calcul numérique installent une prise nouvelle sur la société et le comportement des individus. Elles prétendent « voir » dans les comportements actuels, la forme d’un comportement à venir. Mais pour réaliser cette prédiction, et la rendre opérante, elles doivent installer un dispositif d’enregistrement des données, de calcul des informations et d’orientation des conduites qui n’est pas sans conséquences. Sur quoi reposent la crédibilité et la légitimité de ces machines prédictives ? Comment modifient-elles les manières de connaître ? Sous quelle condition et à quel point le pouvoir se délègue-t-il aux machines ? Illustration de couverture : © Garrett Coakley, “Bomb detail”.},
  langid = {french},
  keywords = {connexionnisme,deep learning,intelligence artificielle,réseaux de neurones,système expert}
}

