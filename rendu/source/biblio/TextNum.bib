@article{lamboyAnalyseLinguisticopragmaTique2005,
  title = {L’analyse Linguistico-Pragma- Tique Des États Mentaux Comme Méthode d’investigation Des Discours Thérapeutiques: {{Pragmatic Linguistic Analysis}} as {{Method}} to {{Investigate}} the {{Therapeutic Speech}}},
  shorttitle = {L’analyse Linguistico-Pragma- Tique Des États Mentaux Comme Méthode d’investigation Des Discours Thérapeutiques},
  author = {Lamboy, Béatrice and Blanchet, Alain and Lecomte, Conrad},
