@online{hudsonGenerativeAdversarialTransformers2022,
  title = {Generative {{Adversarial Transformers}}},
  author = {Hudson, Drew A. and Zitnick, C. Lawrence},
  date = {2022-03-29},
  eprint = {2103.01209},
