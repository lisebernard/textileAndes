  pmcid = {PMC1995127},
  keywords = {Adult,Communication,{Diagnosis, Computer-Assisted},Female,Humans,{Interview, Psychological},Male,Middle Aged,Reference Values,Schizophrenia,Schizophrenic Language,Schizophrenic Psychology,Semantics,Sensitivity and Specificity,Software,Speech Production Measurement,Thinking}
