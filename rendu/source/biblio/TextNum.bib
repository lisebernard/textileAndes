  author = {Nikzad, Amir H. and Cong, Yan and Berretta, Sarah and Hänsel, Katrin and Cho, Sunghye and Pradhan, Sameer and Behbehani, Leily and DeSouza, Danielle D. and Liberman, Mark Y. and Tang, Sunny X.},
  date = {2022-07-05},
  journaltitle = {Schizophrenia},
