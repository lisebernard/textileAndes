@inproceedings{seminckMindItCorpusLongitudinal2020,
  title = {The {{Mind-It Corpus}}: A {{Longitudinal Corpus}} of {{Electronic Messages Written}} by {{Older Adults}} with {{Incipient Alzheimer}}'s {{Disease}} and {{Clinically Normal Volunteers}}},
  shorttitle = {The {{Mind-It Corpus}}},
  author = {Seminck, Olga and Cougnon, Louise-Amélie and Hanseeuw, Bernard and Fairon, Cédrick},
  date = {2020-05-11},
  pages = {108},
  url = {https://hal.science/hal-03106951},
  urldate = {2023-06-14},
  abstract = {In this article, we present the Mind-It project and the corpus we are currently collecting. The long-term aim of the project is to contribute to the preclinical detection of Alzheimer's disease (AD) by developing a computer model that searches for linguistic changes that mark AD. To this end, we will automatically analyze the history of electronic messages, such as those communicated via WhatsApp, Messenger and e-mails, of clinically normal participants and AD patients. The literature about the automatic detection of AD using linguistic input has shown that productions from AD patients are automatically distinguishable from productions of normal older adults. Furthermore, case studies about authors who developed AD themselves suggest that their writing style progressively changes as a result of the disease. With respect to existing corpora containing linguistic materials from AD patients, the data that we collect will form a unique corpus; we are not aware of other resources featuring such longitudinal data. In this article, we argue how our project will contribute to the research on AD and discuss our considerations on collecting, processing and sharing the project's data. We also speculate how the data could be used to develop an automated tool for preclinical detection of AD.},
  eventtitle = {3rd {{RaPID Workshop}}: {{Resources}} and {{Processing}} of {{Linguistic}}, {{Para-linguistic}} and {{Extra-linguistic Data}} from {{People}} with {{Various Form}}},
  langid = {english}
}

@article{seminckUsePatientElectronic2020,
  title = {The Use of Patient's Electronic Message History for the Automatic Preclinical Detection of {{Alzheimer}}'s Disease},
  author = {Seminck, Olga and Hanseeuw, Bernard J. and Cougnon, Louise-Amélie and Fairon, Cédrick},
  date = {2020},
  journaltitle = {Alzheimer's \& Dementia},
  volume = {16},
  number = {S6},
  pages = {e036755},
  issn = {1552-5279},
  doi = {10.1002/alz.036755},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/alz.036755},
  urldate = {2023-06-14},
  abstract = {Background Detecting Alzheimer’s disease (AD) before the onset of symptoms is crucial for the development of effective treatment (1). As biomarkers are costly and/or invasive, there is a need for cost-efficient and non-invasive methods that can indicate which individuals need further examination. We present a running project that aims at developing a screening tool applicable to the general population by automatically analyzing the history of electronic messages such as WhatsApp and e-mails over a period of several years prior to the analysis. Method In the first phase of the project (model construction), 30 prodromal or mildly demented AD patients (with biomarkers-confirmed diagnosis) and 30 clinically normal elderly volunteers are recruited to donate their electronic message histories. Data is gathered and processed according to the General Data Protection Regulation and only sent messages are considered, which are codified to hide personal information. Automatic linguistic analyses are conducted on this data, such as syntactic parsing and lexical analysis and used by machine learning algorithms that establish the evolution of the linguistic abilities of a participant over time. Specifically, we will test how long before AD diagnosis linguistic changes are noticeable versus clinically normal volunteers. In the second phase of the project (model testing), 200 elderly volunteers without AD diagnosis will be recruited to donate their electronic message history and genetically tested for Apolipoprotein E4 (APOE4). We hypothesize that our tool will identify changes in linguistic practices suggestive of preclinical AD more frequently in APOE4 carriers than in non-carriers. Result We are currently collecting and processing data from patients and clinically normal volunteers. We started to run our first linguistic analyses on the data, but we do not have enough participants yet to train a computer model. Conclusion If detection of preclinical AD using linguistic markers from electronic message history turns out to be effective, the tool we develop could play an important role in the non-invasive and cost-efficient screening of preclinical AD in the elderly population. Specifically, it could facilitate clinical trials in preclinical AD. Reference (1) McDade, E. and Bateman, R. J. (2017). Stop Alzheimer’s before it starts. Nature News, 547(7662):153.},
  langid = {english}
}

